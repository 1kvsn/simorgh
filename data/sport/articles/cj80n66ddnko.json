              }
            ]
          }
        },
        {
          "type": "text",
          "model": {
            "blocks": [
              {
                "type": "paragraph",
                "model": {
                  "text": "Great Britain's Lucy Renshall won gold at the Baku Grand Slam by defeating Mongolia's Gankhaich Bold in the final.",
                  "blocks": [
                    {
                      "type": "fragment",
                      "model": {
                        "text": "Great Britain's Lucy Renshall won gold at the Baku Grand Slam by defeating Mongolia's Gankhaich Bold in the final.",
                        "attributes": [
                          "bold"
                        ]
                      }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "type": "social",
          "model": {
            "source": "https://twitter.com/BBCSport/status/1567048705102041091",
            "providerName": "Twitter",
            "blocks": [
              {
                "type": "renditions",
                "model": {
                  "locator": "urn:bbc:optimo:social:4a6cec66-038e-442d-bdd5-404bfcc20290",
                  "blocks": [
                    {
                      "type": "aresOEmbed",
                      "model": {
                        "oembed": {
                          "version": "1.0",
                          "author_name": "BBC Sport",
